{
    "base_url": "http://127.0.0.1:5005/api/v1",
    "callback": {
        "port": 8804,
        "route": "/"
    },
    "setup_test_cases": [
        {
            "store_response_variables": {
                "group_id": "data.uid"
            },
            "http_method": "POST",
            "endpoint": "/groups",
            "request_body": {
                "config": {
                    "disableEndpoint": true,
                    "signature": {
                        "hash": "SHA256",
                        "header": "X-Retro-Signature"
                    },
                    "strategy": {
                        "default": {
                            "intervalSeconds": 30,
                            "retryLimit": 4
                        },
                        "type": "default"
                    }
                },
                "logo_url": "",
                "name": "retro-group-16"
            },
            "response_body": true
        },
        {
            "store_response_variables": {
                "app_id": "data.uid"
            },
            "http_method": "POST",
            "endpoint": "/applications?groupID={group_id}",
            "request_body": {
                "name": "retro-app-16",
                "support_email": "retro_app@gmail.com"
            },
            "response_body": true
        },
        {
            "store_response_variables": {
                "endpoint_id_1": "data.uid"
            },
            "http_method": "POST",
            "endpoint": "/applications/{app_id}/endpoints",
            "request_body": {
                "url": "https://b10e-102-219-153-88.eu.ngrok.io",
                "secret": "12345",
                "description": "Local ngrok endpoint",
                "events": [
                    "payment.failed"
                ]
            },
            "response_body": true
        },
        {
            "store_response_variables": {
                "endpoint_id_2": "data.uid"
            },
            "http_method": "POST",
            "endpoint": "/applications/{app_id}/endpoints",
            "request_body": {
                "url": "https://b10e-102-219-153-88.eu.ngrok.io",
                "secret": "12345",
                "description": "Local ngrok endpoint",
                "events": [
                    "*"
                ]
            },
            "response_body": true
        }
    ],
    "test_cases": [
        {
            "http_method": "POST",
            "endpoint": "/events?groupID={group_id}",
            "callback": {
                "enabled": true,
                "times": 2
            },
            "request_body": {
                "app_id": "{app_id}",
                "event_type": "payment.failed",
                "data": {
                    "sc": "gene",
                    "marvel": "stark"
                }
            },
            "response_body": true
        }
    ]
}